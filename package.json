{
  "name": "kinvey-phonegap-sdk",
  "version": "3.0.3",
  "description": "Kinvey JavaScript SDK for PhoneGap/Cordova applications.",
  "homepage": "http://www.kinvey.com",
  "bugs": {
    "url": "https://github.com/Kinvey/phonegap-sdk/issues"
  },
  "license": "Apache-2.0",
  "author": "Kinvey",
  "contributors": [
    "Thomas Conner <thomas@kinvey.com>"
  ],
  "main": "./dist/export.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:Kinvey/phonegap-sdk"
  },
  "scripts": {
    "build": "gulp build",
    "bundle": "gulp bundle",
    "clean": "gulp clean",
    "lint": "gulp lint",
    "test": "npm run test:unit",
    "test:unit": "mocha --compilers js:babel-core/register -r babel-polyfill -s 100 --recursive test/unit/index test/unit",
    "test:unit:watch": "mocha -w --compilers js:babel-core/register -r babel-polyfill -s 100 --recursive test/unit/index test/unit",
    "test:jenkins": "npm run test:unit:jenkins",
    "test:unit:jenkins": "istanbul cover _mocha -- --reporter tap --compilers js:babel-core/register -r babel-polyfill -s 100 --recursive test/unit/index test/unit > test.tap && istanbul report clover",
    "e2e:server": "appium",
    "e2e:prepare:app": "cd ./test/e2e/app && cordova prepare",
    "e2e:build:app:ios": "npm run e2e:prepare:app && npm run bundle && cd ./test/e2e/app && cordova build ios",
    "e2e:test:ios": "npm run e2e:build:app:ios && wdio ./test/e2e/test/wdio.conf.js"
  },
  "dependencies": {
    "es6-promise": "^3.2.1",
    "kinvey-javascript-sdk-core": "^3.0.0",
    "kinvey-html5-sdk": "^3.0.0",
    "lodash": "^4.0.0",
    "parse-headers": "^2.0.0",
    "regenerator-runtime": "^0.9.5"
  },
  "peerDependencies": {
    "kinvey-javascript-sdk-core": "^3.0.0",
    "kinvey-html5-sdk": "^3.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.0.15",
    "babel-register": "^6.9.0",
    "cordova": "^6.3.1",
    "del": "^2.0.2",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.2",
    "eslint-plugin-react": "^5.1.1",
    "expect": "^1.20.2",
    "fs-extra": "^0.30.0",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.2",
    "gulp-banner": "^0.1.3",
    "gulp-buffer": "0.0.2",
    "gulp-bump": "^2.1.0",
    "gulp-eslint": "^2.0.0",
    "gulp-plumber": "^1.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-s3-upload": "^1.6.0",
    "gulp-tag-version": "^1.3.0",
    "gulp-uglify": "^1.5.1",
    "gulp-util": "^3.0.6",
    "istanbul": "^0.4.3",
    "json-loader": "^0.5.4",
    "mocha": "^2.5.3",
    "nock": "^8.0.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "wdio-dot-reporter": "0.0.6",
    "wdio-mocha-framework": "^0.4.0",
    "wdio-spec-reporter": "0.0.3",
    "webdriverio": "^4.2.5",
    "webpack": "^1.13.0",
    "webpack-stream": "^3.1.0",
    "yargs": "^4.7.1"
  },
  "engines": {
    "node": ">=4.0"
  },
  "keywords": [
    "Kinvey",
    "JavaScript",
    "PhoneGap",
    "Cordova"
  ]
}
