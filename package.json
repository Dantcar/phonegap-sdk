{
  "name": "kinvey-phonegap-sdk",
  "version": "3.1.1",
  "description": "Kinvey JavaScript SDK for PhoneGap/Cordova applications.",
  "homepage": "http://www.kinvey.com",
  "bugs": {
    "url": "https://github.com/Kinvey/phonegap-sdk/issues"
  },
  "license": "Apache-2.0",
  "author": "Kinvey",
  "contributors": [
    "Thomas Conner <thomas@kinvey.com>"
  ],
  "main": "./dist/export.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:Kinvey/phonegap-sdk"
  },
  "scripts": {
    "build": "npm run clean && npm run transpile && npm run bundle && npm run minify",
    "bundle": "webpack",
    "clean": "del coverage dist s3",
    "minify": "uglifyjs --compress warnings=false --comments '/kinvey-phonegap-sdk/' --output ./dist/kinvey-phonegap-sdk.min.js -- ./dist/kinvey-phonegap-sdk.js ",
    "cover": "istanbul cover _mocha -- --compilers js:babel-core/register -r babel-polyfill -s 100 --recursive test/unit/setup test/unit",
    "docs": "esdoc -c esdoc.json",
    "e2e:server": "appium",
    "e2e:prepare:app": "cd ./test/e2e/app && cordova prepare",
    "e2e:build:app:ios": "npm run e2e:prepare:app && npm run build && cd ./test/e2e/app && cordova build ios",
    "e2e:test:ios": "npm run e2e:build:app:ios && wdio ./test/e2e/test/wdio.conf.js",
    "lint": "npm run lint:src",
    "lint:src": "eslint src/**",
    "lint:test": "eslint test/unit/**",
    "preversion": "npm test",
    "postversion": "git push && git push --tags",
    "s3": "shjs ./scripts/before_deploy.js",
    "test": "mocha --compilers js:babel-core/register -r babel-polyfill -s 100 --recursive test/unit/setup test/unit",
    "test:watch": "mocha -w --compilers js:babel-core/register -r babel-polyfill -s 100 --recursive test/unit/setup test/unit",
    "transpile": "babel src --out-dir dist",
    "version": "npm run build && git add -A dist"
  },
  "dependencies": {
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.0",
    "babel-plugin-inline-dotenv": "^1.1.1",
    "babel-polyfill": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.0.15",
    "babel-register": "^6.9.0",
    "cordova": "^6.3.1",
    "core-js": "^2.4.1",
    "del-cli": "^0.2.0",
    "eslint": "^3.5.0",
    "eslint-config-airbnb-base": "^7.1.0",
    "eslint-plugin-import": "^1.15.0",
    "expect": "^1.20.2",
    "istanbul": "^1.0.0-alpha.2",
    "json-loader": "^0.5.4",
    "kinvey-html5-sdk": "3.1.1",
    "kinvey-javascript-sdk-core": "3.1.2",
    "lodash": "^4.8.2",
    "mocha": "^2.5.3",
    "nock": "^8.0.0",
    "parse-headers": "^2.0.1",
    "regenerator-runtime": "^0.9.5",
    "shelljs": "^0.7.4",
    "uglify-js": "^2.7.3",
    "wdio-dot-reporter": "0.0.6",
    "wdio-mocha-framework": "^0.4.0",
    "wdio-spec-reporter": "0.0.3",
    "webdriverio": "^4.2.5",
    "webpack": "^1.13.0"
  },
  "peerDependencies": {
    "kinvey-javascript-sdk-core": "3.1.2",
    "kinvey-html5-sdk": "3.1.1"
  },
  "engines": {
    "node": ">=4.0"
  },
  "keywords": [
    "Kinvey",
    "JavaScript",
    "PhoneGap",
    "Cordova"
  ]
}
